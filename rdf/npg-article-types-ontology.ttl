    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :brief-communications-arising article-type denotes a peer-reviewed communication published in response to a previously published research article."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Brief Communications Arising"@en .

article-types:case-based-learning
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "case-based-learning" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 3 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:case-report ;
    skos:definition "The :case-based-learning article-type denotes a case report with a particular educational focus."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Case-based Learning"@en .

article-types:case-report
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "case-report" ;
    npg:isLeaf false ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :case-report article-type denotes a detailed report of the symptoms, signs, diagnosis, treatment, and follow-up of an individual patient. A case report may contain a demographic profile of the patient and usually describes an unusual or novel occurrence. Some case reports also contain a literature review of other reported cases."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:narrower article-types:case-based-learning, article-types:clinical-cases ;
    skos:prefLabel "Case Report"@en .

article-types:clinical-cases
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "clinical-cases" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 3 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:case-report ;
    skos:definition "The :clinical-cases article-type denotes a report, which describes the findings, clinical course, and prognosis of a case. The :clinical-cases article-type is an alternative to the :case-report article-type."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Clinical Cases"@en .

article-types:comment
    npg:hasRoot article-types:news-and-comment ;
    npg:id "comment" ;
    npg:isLeaf false ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:comments-and-opinion ;
    skos:definition "The :comment article-type generally denotes an agenda-setting, authoritative, informed and often provocative expert piece calling for action on topical issues pertaining to scientific research and its political, ethical and social ramifications. They are written to be accessible and appealing to the entire target readership of the journal and propose a detailed solution rather than simply providing a snapshot of the problem. Alternatively the :comment article-type can denote a writerly historical narrative or conceptual or philosophical argument of pressing contemporary relevance, told with authority, colour, vivacity and personal voice. These attempt to bring an original perspective before the widest readership, through erudite reasoning and telling examples."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:narrower article-types:building-a-business, article-types:patent ;
    skos:prefLabel "Comment"@en .

article-types:comments-and-opinion
    npg:hasRoot article-types:news-and-comment ;
    npg:id "comments-and-opinion" ;
    npg:isLeaf false ;
    npg:isPrimaryArticleType true ;
    npg:isRoot false ;
    npg:treeDepth 1 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:news-and-comment ;
    skos:definition "The :comments-and-opinion article-type groups together article-types including the :comment, :obituary, :q-and-a article-types, etc."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:narrower article-types:comment, article-types:crossword, article-types:fiction, article-types:obituary, article-types:q-and-a ;
    skos:prefLabel "Comments and Opinion"@en .

article-types:conference-proceeding
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "conference-proceeding" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :conference-proceeding article-type denotes an article of original research published in the context of an academic conference."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Conference Proceeding"@en .

article-types:consensus-statement
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "consensus-statement" ;
    npg:isLeaf false ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:reviews ;
    skos:definition "The :consensus-statement article-type denotes an article with the form of a diagnosis/treatment/management guideline (usually society-endorsed), expert consensus document, position paper, scientific statement, standards paper, or white paper. A consensus statement should present a balanced review put together by a taskforce of people from various institutes, rather than from one group, and can be evidence-based or eminence-based. It represents a consensus recommendation."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:narrower article-types:white-paper ;
    skos:prefLabel "Consensus Statement"@en .

article-types:correction
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "correction" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:amendments-and-corrections ;
    skos:definition "The :correction article-type denotes a notification of errors in less scholarly article-types (e.g., :editorial, :comment, :correspondence, :news-and-views) compared with the :erratum and :corrigendum article-types which are used to indicate errors related to primary research. A correction might, for instance, be used to denote inaccuracies in author names and affiliations, errors that affect the credit given to a person or damage someoneâ€™s reputation, or significant misquotations."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Correction"@en .

article-types:correspondence
    npg:hasRoot article-types:news-and-comment ;
    npg:id "correspondence" ;
    npg:isLeaf true ;
    npg:isPrimaryArticleType true ;
    npg:isRoot false ;
    npg:treeDepth 1 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:news-and-comment ;
    skos:definition "The :correspondence article-type denotes arresting and timely comments on material published in the journal (including right-of-reply) as well as anything of likely interest to the readers, such as policy debates and community announcements."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Correspondence"@en .

article-types:corrigendum
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "corrigendum" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:amendments-and-corrections ;
    skos:definition "The :corrigendum article-type denotes a notification of an important error made by the author(s) that affects the publication record or the scientific integrity of the paper, or the reputation of the authors or the journal. In cases where co-authors disagree, the editors will take advice from independent peer-reviewers and impose the appropriate amendment, noting the dissenting author(s) in the text of the published version."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Corrigendum"@en .

article-types:crossword
    npg:hasRoot article-types:news-and-comment ;
    npg:id "crossword" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:comments-and-opinion ;
    skos:definition "The :crossword article-type denotes a word puzzle, usually focused on a particular theme relevant to the target readership."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Crossword"@en .

article-types:data-descriptor
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "data-descriptor" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :data-descriptor article-type denotes an article describing one or more unprocessed experimental or observational data sets, with a strong emphasis on the adequacy of the methods used to collect the data and technical analysis to support its suitability for re-use by others. A data descriptor will never describe tests of new hypotheses or include extensive analyses aimed at providing conceptual insights."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Data Descriptor"@en .

article-types:editorial
    npg:hasRoot article-types:news-and-comment ;
    npg:id "editorial" ;
    npg:isLeaf false ;
    npg:isPrimaryArticleType true ;
    npg:isRoot false ;
    npg:treeDepth 1 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:news-and-comment ;
    skos:definition "The :editorial article-type denotes an article conveying an authoritative opinion that will, at the least, crystallize a current issue of concern and, at best, inspire action by key individuals or groups. Editorial articles are the voice of the journal. An editorial can also deliver an introduction or overview of the content of an issue."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Editorial"@en .

article-types:erratum
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "erratum" ;
    npg:isLeaf false ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:amendments-and-corrections ;
    skos:definition "The :erratum article-type denotes a notification of an important error made by the journal that affects the publication record or the scientific integrity of the paper, or the reputation of the authors, or of the journal."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Erratum"@en .

article-types:feature
    npg:hasRoot article-types:miscellaneous ;
    npg:id "feature" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:special-features ;
    skos:definition "The :feature article-type denotes an article of long-form journalism that provides an in-depth account of new ideas and approaches in science, the dynamics of the research community, and interactions between science and society. Intended to complement the emerging scientific developments reported in research articles, this format provides â€˜insideâ€™ access to topical subjects in science and scientific research: how it works, its challenges and its controversies."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Feature"@en .

article-types:fiction
    npg:hasRoot article-types:news-and-comment ;
    npg:id "fiction" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:comments-and-opinion ;
    skos:definition "The :fiction article-type denotes a fictional self-contained story, typically with a stronger scientific basis than fiction characteristic of the outright fantasy, slipstream, or horror genres."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Fiction"@en .

article-types:hypothesis
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "hypothesis" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :hypothesis article-type denotes a proposal that is consistent with published literature, but for which empirical evidence has not yet been obtained. The purpose would be to inspire experimental research."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Hypothesis"@en .

article-types:image
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "image" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :image article-type denotes a striking photograph or composite figure, accompanied by a limited amount of explanatory text, which highlights a notable phenomenon likely to be of general interest to the target readership."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
