article-types:analysis
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "analysis" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :analysis article-type denotes an article describing analysis of existing data (typically large data sets from high-throughput platforms) or new data obtained through comparative analysis of technologies, methods or reagents of key importance to the field of research. The conclusions should be sufficiently novel and arresting to appeal to a substantial sector of the target audience."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Analysis"@en .

article-types:article
    npg:hasRoot article-types:research-and-reviews ;
    npg:id "article" ;
    npg:isLeaf false ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:research ;
    skos:definition "The :article article-type denotes a relatively complete, comprehensive report of original research. An article usually has a fairly complex narrative that is based on multiple techniques and/or approaches."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:narrower article-types:letter ;
    skos:prefLabel "Article"@en .

article-types:blog-post
    npg:hasRoot article-types:miscellaneous ;
    npg:id "blog-post" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 2 ;
    a npg:ArticleType , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/article-types/> ;
    skos:broader article-types:blogs ;
    skos:definition "The :blog-post article-type denotes an article published on a blog. A blog is an internet-based discussion or informational site that comprises discrete entries (blog posts) typically displayed in reverse chronological order."@en ;
    skos:inScheme <http://ns.nature.com/article-types/> ;
    skos:prefLabel "Blog Post"@en .

