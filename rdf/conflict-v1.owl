    </owl:ObjectProperty>
    


    <!-- http://purl.com/net/conflict#has_items -->

    <owl:ObjectProperty rdf:about="#has_items">
        <rdfs:domain rdf:resource="#Museum_collection"/>
        <rdfs:range rdf:resource="#Physical"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.com/net/conflict#used_instrument -->

    <owl:ObjectProperty rdf:about="#used_instrument">
        <rdfs:domain rdf:resource="#Measurement"/>
        <rdfs:range rdf:resource="#Measuring_Device"/>
    </owl:ObjectProperty>
    


