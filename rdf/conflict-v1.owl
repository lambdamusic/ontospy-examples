<rdf:RDF xmlns="http://purl.com/net/conflict#"
     xml:base="http://purl.com/net/conflict"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:conflict="http://purl.com/net/conflict#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo rdf:datatype="&rdf;XMLLiteral"
            >0.1: created an initial taxonomy from various sources; added inheritance relations with CIDOC-CRM</owl:versionInfo>
        <dc:title rdf:datatype="&rdf;XMLLiteral"
            >Conflict: an ontology for historical modeling of  WW1</dc:title>
        <dc:description
            >The Conflict ontology is an extension of CIDOC-CRM that serves to characterizes the First World War. </dc:description>
        <dc:creator
            >Michele Pasin in the context of the SAILS project [www.michelepasin.org] [http://sailsproject.cerch.kcl.ac.uk/]</dc:creator>
        <owl:imports rdf:resource="http://www.cidoc-crm.org/rdfs/cidoc_crm_v5.0.2_english_label.rdfs"/>
    </owl:Ontology>
    


