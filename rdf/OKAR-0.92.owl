    <rdfs:range  rdf:resource="&xsd;string" />
    <rdfs:isDefinedBy rdf:resource="&dc;" />
  </owl:DatatypeProperty>

  <!-- Source (DC) 2004-11-10 -->
  <owl:ObjectProperty rdf:about="&dc;source">
