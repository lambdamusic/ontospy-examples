    <rdfs:label xml:lang="en">Related Event</rdfs:label>
    <rdfs:label xml:lang="ja">関連イベント</rdfs:label>
    <rdfs:comment xml:lang="en">The "okar:relatedEvent" property represents a relationship between okar:Event.</rdfs:comment>
    <rdfs:comment xml:lang="ja">okar:relatedEventプロパティは、okar:Event間の関係を記述するプロパティです。</rdfs:comment>
    <rdfs:domain rdf:resource="#Event" />
    <rdfs:range  rdf:resource="#Event" />
    <rdfs:isDefinedBy rdf:resource="&okarURI;" /> 
  </owl:ObjectProperty>

  <!-- User (OKAR) 2005-03-31 -->
