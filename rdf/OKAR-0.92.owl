    <rdfs:range  rdf:resource="#Artifact"/>
    <rdfs:isDefinedBy rdf:resource="&dc;" />
  </owl:ObjectProperty>

  <!-- Related Artifact (DC) 2004-11-10 -->
  <owl:ObjectProperty rdf:about="&dc;relation"> 
