    <rdfs:range  rdf:resource="#Artifact"/>
    <rdfs:isDefinedBy rdf:resource="&dc;" />
  </owl:ObjectProperty>

  <!-- Artifact Identifier (DC) 2004-11-10 -->
  <owl:DatatypeProperty rdf:about="&dc;identifier"> 
    <rdfs:domain rdf:resource="#Artifact" />
    <rdfs:range  rdf:resource="&xsd;anyURI" />
    <rdfs:isDefinedBy rdf:resource="&dc;" />
  </owl:DatatypeProperty>

  <!-- Artifact Type: Genre (DC) 2004-11-10 -->
  <owl:ObjectProperty rdf:about="&dc;type"> 
    <rdfs:domain rdf:resource="#Artifact" />
    <!-- <rdfs:range  rdf:resource="&dcterms;DCMIType" /> -->
    <rdfs:comment xml:lang="en"> NOTE: DCMI recommended best practice is to select a value from a controlled vocabulary (for example, the DCMI Type Vocabulary).</rdfs:comment>
    <rdfs:comment xml:lang="ja">［注意］DCMIでは、例えば、DCMI Type Vocabularyのような制限言語の利用を推奨しています。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&dc;" />
